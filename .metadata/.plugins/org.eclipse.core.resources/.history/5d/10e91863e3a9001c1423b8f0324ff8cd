
public class NoLista<T> {
	private T info;
	private NoLista<T> proximo;
	
	public int getInfo() {
		return info;
	}
	
	public void setInfo(int info) {
		this.info = info;
	}
	
	public NoLista<T> getProximo() {
		return proximo;
	}
	
	public void setProximo(NoLista<T> proximo) {
		this.proximo = proximo;
	}
	
}
